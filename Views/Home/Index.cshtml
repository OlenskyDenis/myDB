@model List<Publication>
@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1>Список публікаций</h1>

    @foreach (var publication in Model)
    {
        <ul>
            <li><a href="Publications/Details/@publication.Id">@publication.Title</a></li>
        </ul>
    }
</div>
